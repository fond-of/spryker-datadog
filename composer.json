{
  "name": "fond-of-spryker/datadog",
  "description": "Datadog module",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "spryker/monitoring-extension": "^1.0.0",
    "datadog/dd-trace": "^0.34.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "fond-of-codeception/spryker": "dev-master as 1.0.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/"
    }
  }
}
